<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".FeedArtistaFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imgFotoPerfilPequena"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="15dp"
                android:contentDescription="@string/perfil"
                android:src="@drawable/vitor" />

            <TextView
                android:id="@+id/txtNomeUsuario"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="10dp"
                android:text="@string/vitor_messias"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="normal" />

            <TextView
                android:id="@+id/txtSP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:text="@string/sao_paulo_sp"
                android:textColor="#000"
                android:textSize="12sp"
                android:textStyle="normal" />

            <ImageView
                android:id="@+id/imgFotoPontos"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="348dp"
                android:layout_marginTop="20dp"
                android:contentDescription="@string/opcoes"
                android:src="@drawable/pontos" />

            <ImageView
                android:id="@+id/imgFotoPostPerfil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/txtSP"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="60dp"
                android:contentDescription="@string/foto"
                android:src="@drawable/gusta" />

            <ImageView
                android:id="@+id/imgFotoPerfilPequena2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="320dp"
                android:contentDescription="@string/perfil"
                android:src="@drawable/robert" />

            <TextView
                android:id="@+id/txtNomeUsuario2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="315dp"
                android:text="@string/robert_taylor"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="normal" />

            <TextView
                android:id="@+id/txtSP2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="335dp"
                android:text="@string/sao_paulo_sp"
                android:textColor="#000"
                android:textSize="12sp"
                android:textStyle="normal" />

            <ImageView
                android:id="@+id/imgFotoPontos2"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="348dp"
                android:layout_marginTop="320dp"
                android:contentDescription="@string/opcoes"
                android:src="@drawable/pontos" />

            <ImageView
                android:id="@+id/imgFotoPostPerfil2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/txtSP"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="360dp"
                android:contentDescription="@string/foto"
                android:src="@drawable/gusta" />

            <ImageView
                android:id="@+id/imgFotoPerfilPequena3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="620dp"
                android:contentDescription="@string/perfil"
                android:src="@drawable/gustavo" />

            <TextView
                android:id="@+id/txtNomeUsuario3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="615dp"
                android:text="@string/gustavo_araujo"
                android:textColor="#000"
                android:textSize="18sp"
                android:textStyle="normal" />

            <TextView
                android:id="@+id/txtSP3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="40dp"
                android:layout_marginTop="635dp"
                android:text="@string/sao_paulo_sp"
                android:textColor="#000"
                android:textSize="12sp"
                android:textStyle="normal" />

            <ImageView
                android:id="@+id/imgFotoPontos3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignStart="@+id/imgFotoPerfilPequena"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="348dp"
                android:layout_marginTop="620dp"
                android:contentDescription="@string/opcoes"
                android:src="@drawable/pontos" />

            <ImageView
                android:id="@+id/imgFotoPostPerfil3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignStart="@+id/txtSP"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="660dp"
                android:contentDescription="@string/foto"
                android:src="@drawable/gusta" />
        </RelativeLayout>
    </ScrollView>
</FrameLayout>